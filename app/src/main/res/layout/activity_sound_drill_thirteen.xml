<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="match_parent"
    android:background="@drawable/backgroundwriteletters"
    tools:context="classact.com.xprize.activity.drill.sound.SoundDrillThirteenActivity">
    <LinearLayout
        android:layout_marginLeft="120dip"
        android:layout_width="150dip"
        android:id="@+id/container1"
        android:layout_height="150dip"
        android:orientation="vertical"
        android:layout_marginStart="20dp"
        android:layout_alignTop="@+id/container3"
        android:layout_alignParentStart="true" />
    <LinearLayout
        android:layout_below="@+id/container1"
        android:layout_marginLeft="120dip"
        android:layout_marginTop="50dp"
        android:layout_width="150dip"
        android:orientation="vertical"
        android:id="@+id/container2"
        android:layout_height="150dip" />
    <LinearLayout
        android:layout_toRightOf="@+id/container1"
        android:layout_marginLeft="120dip"
        android:layout_marginTop="10dp"
        android:layout_width="150dip"
        android:orientation="vertical"
        android:id="@+id/container3"
        android:layout_height="150dip" />
    <LinearLayout
        android:layout_toRightOf="@+id/container2"
        android:layout_below="@+id/container3"
        android:layout_marginLeft="120dip"
        android:layout_marginTop="50dp"
        android:layout_width="150dip"
        android:orientation="vertical"
        android:id="@+id/container4"
        android:layout_height="150dip" />
    <LinearLayout
        android:layout_toRightOf="@+id/container3"
        android:layout_marginLeft="120dip"
        android:layout_marginTop="10dp"
        android:layout_width="150dip"
        android:id="@+id/container5"
        android:orientation="vertical"
        android:layout_height="150dip" />
    <LinearLayout
        android:layout_toRightOf="@+id/container4"
        android:layout_below="@+id/container5"
        android:layout_marginLeft="120dip"
        android:layout_marginTop="50dp"
        android:layout_width="150dip"
        android:id="@+id/container6"
        android:orientation="vertical"
        android:layout_height="150dip" />
    <LinearLayout
        android:layout_toRightOf="@+id/container5"
        android:layout_marginLeft="120dip"
        android:layout_marginTop="10dp"
        android:layout_width="150dip"
        android:id="@+id/container7"
        android:orientation="vertical"
        android:layout_height="150dip" />
    <LinearLayout
        android:layout_toRightOf="@+id/container6"
        android:layout_below="@+id/container7"
        android:layout_marginLeft="120dip"
        android:layout_marginTop="50dp"
        android:layout_width="150dip"
        android:id="@+id/container8"
        android:orientation="vertical"
        android:layout_height="150dp" />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_below="@+id/container1"
        android:layout_marginTop="300dp"
        android:orientation="horizontal"
        android:layout_height="wrap_content">
        <ImageView
            android:layout_marginLeft="260dip"
            android:layout_width="100dip"
            android:id="@+id/loc1"
            android:src="@drawable/line"
            android:contentDescription="@string/app_name"
            android:layout_height="100dip" />
        <ImageView
            android:layout_marginLeft="10dip"
            android:layout_width="100dip"
            android:id="@+id/loc2"
            android:src="@drawable/line"
            android:contentDescription="@string/app_name"
            android:layout_height="100dip" />
        <ImageView
            android:layout_marginLeft="10dip"
            android:layout_width="100dip"
            android:id="@+id/loc3"
            android:src="@drawable/line"
            android:contentDescription="@string/app_name"
            android:layout_height="100dip" />
        <ImageView
            android:layout_marginLeft="10dip"
            android:layout_width="100dip"
            android:id="@+id/loc4"
            android:src="@drawable/line"
            android:contentDescription="@string/app_name"
            android:layout_height="100dip" />
        <ImageView
            android:layout_marginLeft="10dip"
            android:layout_width="100dip"
            android:id="@+id/loc5"
            android:src="@drawable/line"
            android:contentDescription="@string/app_name"
            android:layout_height="100dip" />
        <ImageView
            android:layout_marginLeft="10dip"
            android:layout_width="100dip"
            android:id="@+id/loc6"
            android:src="@drawable/line"
            android:contentDescription="@string/app_name"
            android:layout_height="100dip" />
        <ImageView
            android:layout_marginLeft="10dip"
            android:layout_width="100dip"
            android:id="@+id/loc7"
            android:src="@drawable/line"
            android:contentDescription="@string/app_name"
            android:layout_height="100dip" />
        <ImageView
            android:layout_marginLeft="10dip"
            android:layout_width="100dip"
            android:id="@+id/loc8"
            android:src="@drawable/line"
            android:contentDescription="@string/app_name"
            android:layout_height="100dp" />
    </LinearLayout>
</RelativeLayout>
