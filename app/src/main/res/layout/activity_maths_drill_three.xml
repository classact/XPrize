<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_maths_drill_three"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@drawable/background_monkey_wants_to_eat"
    tools:context="za.co.xprize.activity.mathdrill.MathsDrillThreeActivity">
    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_marginTop="450dip"
        android:id="@+id/itemsReceptacle"
        android:layout_height="wrap_content">
        <ImageView
            android:layout_width="145dip"
            android:layout_height="145dip"
            android:contentDescription="@string/app_name"
            android:id="@+id/item_one"
            android:visibility="invisible"
            android:src="@drawable/cupcake" />
        <ImageView
            android:layout_width="145dip"
            android:layout_height="145dip"
            android:contentDescription="@string/app_name"
            android:id="@+id/item_two"
            android:visibility="invisible"
            android:layout_toRightOf="@+id/item_one"
            android:src="@drawable/cupcake" />
        <ImageView
            android:layout_width="145dip"
            android:layout_height="145dip"
            android:contentDescription="@string/app_name"
            android:id="@+id/item_three"
            android:visibility="invisible"
            android:layout_toRightOf="@+id/item_two"
            android:src="@drawable/cupcake" />
        <ImageView
            android:layout_width="145dip"
            android:layout_height="145dip"
            android:contentDescription="@string/app_name"
            android:id="@+id/item_four"
            android:visibility="invisible"
            android:layout_toRightOf="@+id/item_three"
            android:src="@drawable/cupcake" />
        <ImageView
            android:layout_width="145dip"
            android:layout_height="145dip"
            android:contentDescription="@string/app_name"
            android:id="@+id/item_five"
            android:visibility="invisible"
            android:layout_toRightOf="@+id/item_four"
            android:src="@drawable/cupcake" />
        <ImageView
            android:layout_width="145dip"
            android:layout_height="145dip"
            android:layout_below="@+id/item_one"
            android:visibility="invisible"
            android:contentDescription="@string/app_name"
            android:id="@+id/item_six"
            android:src="@drawable/cupcake" />
        <ImageView
            android:layout_width="145dip"
            android:layout_height="145dip"
            android:layout_toRightOf="@id/item_six"
            android:contentDescription="@string/app_name"
            android:id="@+id/item_seven"
            android:layout_below="@+id/item_two"
            android:visibility="invisible"
            android:src="@drawable/cupcake" />
        <ImageView
            android:layout_width="145dip"
            android:layout_height="145dip"
            android:contentDescription="@string/app_name"
            android:id="@+id/item_eight"
            android:layout_below="@+id/item_three"
            android:visibility="invisible"
            android:layout_toRightOf="@id/item_seven"
            android:src="@drawable/cupcake" />
        <ImageView
            android:layout_width="145dip"
            android:layout_height="145dip"
            android:contentDescription="@string/app_name"
            android:id="@+id/item_nine"
            android:layout_below="@+id/item_four"
            android:visibility="invisible"
            android:layout_toRightOf="@id/item_eight"
            android:src="@drawable/cupcake" />
        <ImageView
            android:layout_width="145dip"
            android:layout_height="145dip"
            android:contentDescription="@string/app_name"
            android:id="@+id/item_ten"
            android:layout_below="@+id/item_five"
            android:visibility="invisible"
            android:layout_toRightOf="@id/item_nine"
            android:src="@drawable/cupcake" />
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_marginLeft="850dip"
        android:id="@+id/itemsContainer"
        android:layout_height="wrap_content">
        <ImageView
            android:layout_width="145dip"
            android:layout_height="145dip"
            android:contentDescription="@string/app_name"
            android:id="@+id/item1"
            android:visibility="invisible"
            android:src="@drawable/cupcake" />
        <ImageView
            android:layout_width="145dip"
            android:layout_height="145dip"
            android:contentDescription="@string/app_name"
            android:id="@+id/item2"
            android:layout_below="@+id/item1"
            android:visibility="invisible"
            android:src="@drawable/cupcake" />
        <ImageView
            android:layout_width="145dip"
            android:layout_height="145dip"
            android:contentDescription="@string/app_name"
            android:id="@+id/item3"
            android:layout_below="@+id/item2"
            android:visibility="invisible"
            android:src="@drawable/cupcake" />
        <ImageView
            android:layout_width="145dip"
            android:layout_height="145dip"
            android:contentDescription="@string/app_name"
            android:id="@+id/item4"
            android:layout_below="@+id/item3"
            android:visibility="invisible"
            android:src="@drawable/cupcake" />
        <ImageView
            android:layout_width="145dip"
            android:layout_height="145dip"
            android:contentDescription="@string/app_name"
            android:id="@+id/item5"
            android:layout_below="@+id/item4"
            android:visibility="invisible"
            android:src="@drawable/cupcake" />
        <ImageView
            android:layout_width="145dip"
            android:layout_height="145dip"
            android:layout_toRightOf="@id/item1"
            android:contentDescription="@string/app_name"
            android:id="@+id/item6"
            android:visibility="invisible"
            android:src="@drawable/cupcake" />
        <ImageView
            android:layout_width="145dip"
            android:layout_height="145dip"
            android:layout_toRightOf="@id/item2"
            android:contentDescription="@string/app_name"
            android:id="@+id/item7"
            android:visibility="invisible"
            android:layout_below="@+id/item6"
            android:src="@drawable/cupcake" />
        <ImageView
            android:layout_width="145dip"
            android:layout_height="145dip"
            android:visibility="invisible"
            android:contentDescription="@string/app_name"
            android:id="@+id/item8"
            android:layout_below="@+id/item7"
            android:layout_toRightOf="@id/item1"
            android:src="@drawable/cupcake" />
        <ImageView
            android:layout_width="145dip"
            android:layout_height="145dip"
            android:visibility="invisible"
            android:contentDescription="@string/app_name"
            android:id="@+id/item9"
            android:layout_below="@+id/item8"
            android:layout_toRightOf="@id/item1"
            android:src="@drawable/cupcake" />
        <ImageView
            android:layout_width="145dip"
            android:layout_height="145dip"
            android:contentDescription="@string/app_name"
            android:id="@+id/item10"
            android:layout_below="@+id/item9"
            android:visibility="invisible"
            android:layout_toRightOf="@id/item1"
            android:src="@drawable/cupcake" />
    </RelativeLayout>
</RelativeLayout>
