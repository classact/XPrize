<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_maths_drill_six_and_four"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="0dip"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@drawable/background_dama_monkey_outside"
    tools:context="classact.com.xprize.activity.drill.math.MathsDrillSixAndFourActivity">
    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_marginTop="500dip"
        android:id="@+id/damasItems"
        android:layout_height="wrap_content">
        <ImageView
            android:layout_width="60dip"
            android:layout_height="60dip"
            android:contentDescription="@string/app_name"
            android:id="@+id/item_one"
            android:visibility="invisible"
            android:background="@android:color/transparent"
            android:layout_alignParentTop="true"
            android:layout_alignParentStart="true" />
        <ImageView
            android:layout_width="60dip"
            android:layout_height="60dip"
            android:contentDescription="@string/app_name"
            android:id="@+id/item_two"
            android:visibility="invisible"
            android:background="@android:color/transparent"
            android:layout_alignParentTop="true"
            android:layout_toEndOf="@+id/item_one" />
        <ImageView
            android:layout_width="60dip"
            android:layout_height="60dip"
            android:contentDescription="@string/app_name"
            android:id="@+id/item_three"
            android:visibility="invisible"
            android:background="@android:color/transparent"
            android:layout_alignParentTop="true"
            android:layout_toEndOf="@+id/item_two" />
        <ImageView
            android:layout_width="60dip"
            android:layout_height="60dip"
            android:contentDescription="@string/app_name"
            android:id="@+id/item_four"
            android:visibility="invisible"
            android:background="@android:color/transparent"
            android:layout_alignParentTop="true"
            android:layout_toEndOf="@+id/item_three" />
        <ImageView
            android:layout_width="60dip"
            android:layout_height="60dip"
            android:contentDescription="@string/app_name"
            android:id="@+id/item_five"
            android:visibility="invisible"
            android:background="@android:color/transparent"
            android:layout_toEndOf="@+id/item_four"/>
        <ImageView
            android:layout_width="60dip"
            android:layout_height="60dip"
            android:visibility="invisible"
            android:contentDescription="@string/app_name"
            android:id="@+id/item_six"
            android:background="@android:color/transparent"
            android:layout_below="@id/item_one"/>
        <ImageView
            android:layout_width="60dip"
            android:layout_height="60dip"
            android:contentDescription="@string/app_name"
            android:id="@+id/item_seven"
            android:visibility="invisible"
            android:background="@android:color/transparent"
            android:layout_below="@id/item_one"
            android:layout_toEndOf="@+id/item_six"/>
        <ImageView
            android:layout_width="60dip"
            android:layout_height="60dip"
            android:contentDescription="@string/app_name"
            android:id="@+id/item_eight"
            android:visibility="invisible"
            android:layout_below="@id/item_one"
            android:layout_toRightOf="@id/item_seven"
            android:background="@android:color/transparent" />
        <ImageView
            android:layout_width="60dip"
            android:layout_height="60dip"
            android:contentDescription="@string/app_name"
            android:id="@+id/item_nine"
            android:visibility="invisible"
            android:background="@android:color/transparent"
            android:layout_below="@id/item_one"
            android:layout_toEndOf="@+id/item_eight" />
        <ImageView
            android:layout_width="60dip"
            android:layout_height="60dip"
            android:contentDescription="@string/app_name"
            android:id="@+id/item_ten"
            android:visibility="invisible"
            android:layout_below="@id/item_one"
            android:layout_toEndOf="@+id/item_nine"
            android:background="@android:color/transparent" />
        <ImageView
            android:layout_width="60dip"
            android:layout_height="60dip"
            android:contentDescription="@string/app_name"
            android:id="@+id/item_eleven"
            android:visibility="invisible"
            android:layout_below="@id/item_six"
            android:background="@android:color/transparent" />
        <ImageView
            android:layout_width="60dip"
            android:layout_height="60dip"
            android:contentDescription="@string/app_name"
            android:id="@+id/item_twelve"
            android:visibility="invisible"
            android:background="@android:color/transparent"
            android:layout_toEndOf="@+id/item_eleven"
            android:layout_below="@id/item_six"
             />
        <ImageView
            android:layout_width="60dip"
            android:layout_height="60dip"
            android:contentDescription="@string/app_name"
            android:id="@+id/item_thirteen"
            android:visibility="invisible"
            android:background="@android:color/transparent"
            android:layout_toEndOf="@+id/item_twelve"
            android:layout_below="@id/item_six" />
        <ImageView
            android:layout_width="60dip"
            android:layout_height="60dip"
            android:contentDescription="@string/app_name"
            android:id="@+id/item_fourteen"
            android:visibility="invisible"
            android:background="@android:color/transparent"
            android:layout_toEndOf="@+id/item_thirteen"
            android:layout_below="@id/item_six"/>
        <ImageView
            android:layout_width="60dip"
            android:layout_height="60dip"
            android:contentDescription="@string/app_name"
            android:id="@+id/item_fifteen"
            android:visibility="invisible"
            android:background="@android:color/transparent"
            android:layout_below="@id/item_six"
            android:layout_toEndOf="@+id/item_fourteen" />
    </RelativeLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="500dip"
        android:paddingLeft="50px"
        android:id="@+id/equation_container"
        android:layout_toEndOf="@+id/damasItems"
        android:orientation="horizontal">
        <ImageView
            android:layout_width="50dip"
            android:layout_height="50dip"
            android:contentDescription="@string/app_name"
            android:id="@+id/equation_one"
            android:visibility="invisible"
            android:background="@android:color/transparent" />
        <ImageView
            android:layout_width="50dip"
            android:layout_height="50dip"
            android:contentDescription="@string/app_name"
            android:id="@+id/equation_sign"
            android:visibility="invisible"
            android:background="@android:color/transparent" />
        <ImageView
            android:layout_width="50dip"
            android:layout_height="50dip"
            android:contentDescription="@string/app_name"
            android:id="@+id/equation_two"
            android:visibility="invisible"
            android:background="@android:color/transparent" />
        <ImageView
            android:layout_width="50dip"
            android:layout_height="50dip"
            android:contentDescription="@string/app_name"
            android:id="@+id/equation_equals"
            android:visibility="invisible"
            android:background="@android:color/transparent" />
        <ImageView
            android:layout_width="50dip"
            android:layout_height="50dip"
            android:contentDescription="@string/app_name"
            android:id="@+id/equation_answer"
            android:visibility="invisible"
            android:background="@android:color/transparent" />
    </LinearLayout>
    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_marginTop="500dip"
        android:paddingLeft="50px"
        android:id="@+id/monkeysReceptable"
        android:layout_toEndOf="@+id/equation_container"
        android:layout_height="wrap_content">
        <ImageView
            android:layout_width="60dip"
            android:layout_height="60dip"
            android:contentDescription="@string/app_name"
            android:id="@+id/space_one"
            android:visibility="invisible"
            android:background="@android:color/transparent"
            android:layout_alignParentTop="true"
            android:layout_alignParentStart="true" />
        <ImageView
            android:layout_width="60dip"
            android:layout_height="60dip"
            android:contentDescription="@string/app_name"
            android:id="@+id/space_two"
            android:visibility="invisible"
            android:background="@android:color/transparent"
            android:layout_alignParentTop="true"
            android:layout_toEndOf="@+id/space_one" />
        <ImageView
            android:layout_width="60dip"
            android:layout_height="60dip"
            android:contentDescription="@string/app_name"
            android:id="@+id/space_three"
            android:visibility="invisible"
            android:background="@android:color/transparent"
            android:layout_alignParentTop="true"
            android:layout_toEndOf="@+id/space_two" />
        <ImageView
            android:layout_width="60dip"
            android:layout_height="60dip"
            android:contentDescription="@string/app_name"
            android:id="@+id/space_four"
            android:visibility="invisible"
            android:background="@android:color/transparent"
            android:layout_alignParentTop="true"
            android:layout_toEndOf="@+id/space_three" />
        <ImageView
            android:layout_width="60dip"
            android:layout_height="60dip"
            android:contentDescription="@string/app_name"
            android:id="@+id/space_five"
            android:visibility="invisible"
            android:background="@android:color/transparent"
            android:layout_toEndOf="@+id/space_four"/>
        <ImageView
            android:layout_width="60dip"
            android:layout_height="60dip"
            android:visibility="invisible"
            android:contentDescription="@string/app_name"
            android:id="@+id/space_six"
            android:background="@android:color/transparent"
            android:layout_below="@id/space_one"/>
        <ImageView
            android:layout_width="60dip"
            android:layout_height="60dip"
            android:contentDescription="@string/app_name"
            android:id="@+id/space_seven"
            android:visibility="invisible"
            android:background="@android:color/transparent"
            android:layout_below="@id/space_one"
            android:layout_toEndOf="@+id/space_six"/>
        <ImageView
            android:layout_width="60dip"
            android:layout_height="60dip"
            android:contentDescription="@string/app_name"
            android:id="@+id/space_eight"
            android:visibility="invisible"
            android:layout_below="@id/space_one"
            android:layout_toRightOf="@id/space_seven"
            android:background="@android:color/transparent" />
        <ImageView
            android:layout_width="60dip"
            android:layout_height="60dip"
            android:contentDescription="@string/app_name"
            android:id="@+id/space_nine"
            android:visibility="invisible"
            android:background="@android:color/transparent"
            android:layout_below="@id/space_one"
            android:layout_toEndOf="@+id/space_eight" />
        <ImageView
            android:layout_width="60dip"
            android:layout_height="60dip"
            android:contentDescription="@string/app_name"
            android:id="@+id/space_ten"
            android:visibility="invisible"
            android:layout_below="@id/space_one"
            android:layout_toEndOf="@+id/space_nine"
            android:background="@android:color/transparent" />
        <ImageView
            android:layout_width="60dip"
            android:layout_height="60dip"
            android:contentDescription="@string/app_name"
            android:id="@+id/space_eleven"
            android:visibility="invisible"
            android:layout_below="@id/space_six"
            android:background="@android:color/transparent" />
        <ImageView
            android:layout_width="60dip"
            android:layout_height="60dip"
            android:contentDescription="@string/app_name"
            android:id="@+id/space_twelve"
            android:visibility="invisible"
            android:background="@android:color/transparent"
            android:layout_toEndOf="@+id/space_eleven"
            android:layout_below="@id/space_six"
            />
        <ImageView
            android:layout_width="60dip"
            android:layout_height="60dip"
            android:contentDescription="@string/app_name"
            android:id="@+id/space_thirteen"
            android:visibility="invisible"
            android:background="@android:color/transparent"
            android:layout_toEndOf="@+id/space_twelve"
            android:layout_below="@id/space_six" />
        <ImageView
            android:layout_width="60dip"
            android:layout_height="60dip"
            android:contentDescription="@string/app_name"
            android:id="@+id/space_fourteen"
            android:visibility="invisible"
            android:background="@android:color/transparent"
            android:layout_toEndOf="@+id/space_thirteen"
            android:layout_below="@id/space_six"/>
        <ImageView
            android:layout_width="60dip"
            android:layout_height="60dip"
            android:contentDescription="@string/app_name"
            android:id="@+id/space_fifteen"
            android:visibility="invisible"
            android:background="@android:color/transparent"
            android:layout_below="@id/space_six"
            android:layout_toEndOf="@+id/space_fourteen" />
    </RelativeLayout>
    <RelativeLayout
        android:layout_height="wrap_content"
        android:id="@+id/numbers_container"
        android:layout_toEndOf="@+id/monkeysReceptable"
        android:visibility="invisible"
        android:layout_marginTop="360dip"
        android:layout_marginLeft="50dip"
        android:layout_width="wrap_content">
        <ImageView
            android:layout_width="100dip"
            android:layout_height="100dip"
            android:contentDescription="@string/app_name"
            android:layout_gravity="center_vertical"
            android:id="@+id/numeral_1"
            android:background="@android:color/transparent" />
        <ImageView
            android:layout_width="100dip"
            android:layout_height="100dip"
            android:contentDescription="@string/app_name"
            android:id="@+id/numeral_2"
            android:background="@android:color/transparent"
            android:layout_gravity="center_vertical"
            android:layout_below="@+id/numeral_1"
            android:layout_marginTop="20dp"/>
        <ImageView
            android:layout_width="100dip"
            android:layout_height="100dip"
            android:contentDescription="@string/app_name"
            android:layout_gravity="center_vertical"
            android:id="@+id/numeral_3"
            android:background="@android:color/transparent"
            android:layout_below="@+id/numeral_2"/>
    </RelativeLayout>
</RelativeLayout>
